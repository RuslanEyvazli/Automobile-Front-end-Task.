        <div class="row main-img-wrapper-row">

            <div class="col-lg-12 main-img-wrapper-cont">
                <div class="main-img-wrapper">
                    <img src="~/Public/image/audi.jpg" alt="">

                </div>

            </div>
            <div class="col-lg-12">
                <div class="img-wrapper position-relative">
                    <i class="fas fa-arrow-left position-absolute"></i>
                    <ul>
                        <li class="active"><img src="~/Public/image/audi.jpg" alt=""></li>
                        <li><img src="~/Public/image/mers.jpg" alt=""></li>
                        <li class=""><img src="~/Public/image/range.jpg" alt=""></li>
                        <li class=""><img src="~/Public/image/land.jpg" alt=""></li>
                        <li><img src="~/Public/image/bmw.jpg" alt=""></li>
                        <li><img src="~/Public/image/wosvogen.jpg" alt=""></li>




                    </ul>
                    <i class="fas fa-arrow-right position-absolute"></i>
                </div>

            </div>

        </div>

    </div>



</main>


